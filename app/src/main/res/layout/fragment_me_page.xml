<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MePage">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/workoutPage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        tools:layout_editor_absoluteX="-59dp"
        tools:layout_editor_absoluteY="-80dp"
        tools:visibility="visible">

        <TextView
            android:id="@+id/workoutPageTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="26dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/pt_sans"
            android:text="USER ANALYTICS"
            android:textColor="#000000"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/date" />

        <TextView
            android:id="@+id/date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="26dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/pt_sans"
            android:text="THURSDAY, SEPTEMBER 7"
            android:textColor="#757575"
            android:textSize="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/workoutPageTitle"
            tools:layout_editor_absoluteX="26dp">

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/profilePicture"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_centerInParent="true"
                android:src="@drawable/male_profile"
                app:shapeAppearanceOverlay="@style/circular" />

            <TextView
                android:id="@+id/nameText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/pt_sans"
                android:text="Ernest Khalimov"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="25dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="26dp"
                android:layout_marginEnd="26dp"
                android:background="@drawable/rounded_black"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/currentWeightText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:fontFamily="@font/pt_sans"
                        android:text="75kg"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="30dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/pt_sans"
                        android:text="Weight"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        tools:ignore="TextSizeCheck" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/workoutStreakText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:fontFamily="@font/pt_sans"
                        android:text="56 Weeks"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="30dp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/pt_sans"
                        android:text="Streak"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        tools:ignore="TextSizeCheck" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/pt_sans"
                android:text="Strength Records"
                android:textAlignment="textStart"
                android:textColor="#000000"
                android:textSize="20dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="400dp"
                android:layout_marginTop="3dp"
                android:layout_weight="1"
                android:background="@drawable/blue_white_gradient"
                tools:ignore="ExtraText">>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingStart="26dp"
                    android:paddingEnd="26dp"
                    android:paddingBottom="250dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="26dp"
                        android:background="@drawable/rounded_gold"
                        android:orientation="horizontal"
                        android:paddingBottom="10dp">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestAllTimeText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="110kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="30dp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/highestAllTimeExerciseText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Bench Press"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="italic" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="Heaviest Weight Lifted"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestBenchPressText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="110kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Bench Press"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestBackRowsText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="90kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Back Rows"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestTricepPushdownText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="75kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Tricep Pushdown"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestLatPulldownText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="85kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Lat Pulldown"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestBicepCurlText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="90kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Bicep Curl"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestChestFlyText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="100kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Chest Fly"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestShoulderPressText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="75kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Shoulder Press"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/highestHammerCurlText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/pt_sans"
                                android:text="65kg"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/pt_sans"
                                android:text="Hammer Curl"
                                android:textAlignment="center"
                                android:textColor="#FFFFFF"
                                android:textSize="13dp"
                                tools:ignore="TextSizeCheck" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
            </ScrollView>

        </LinearLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>